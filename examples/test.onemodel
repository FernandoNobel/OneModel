model ProteinConstitutive

  species 
    mRNA = "0"
    protein = "0"
  end
  
  parameter
    k_m = "1"
    d_m = "1"
    k_p = "1"
    d_p = "1"
  end
  
  reaction
    J1: "0" -> "mRNA" ; "k_m"
    J2: "mRNA" -> "0" ; "d_m*mRNA"
    J3: "mRNA" -> "mRNA" + "protein" ; "k_p*mRNA"
    J4: "protein" -> "0" ; "d_p*protein"
  end

end

model ProteinInduced (ProteinConstitutive)

  input 
    TF
  end

  species 
    k_m = "0"
  end
  
  parameter
    h = "1"
    k_m_max = "1"
  end
  
  rule 
    R1: "k_m" := "k_m_max * TF/(TF+h)"
  end

end

function getMass(aProtein)
  return aProtein.protein
end

A = ProteinConstitutive()
B = ProteinInduced()

parameter foo = "bar*2"

rule R1: B.TF := A.protein + {{ getMass(A) }}


