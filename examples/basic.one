model antithetic_controller()
  # Parameters
  k1 = 1
  k2 = 1
  k3 = 1
  d1 = 0
  d2 = 0
  d3 = 1
  gamma12 = 1
  
  # Species
  x1 = 0
  x2 = 0
  x3 = 0
  x4 = 100
  x5 = 0
  
  # Reactions
  -> x1 ; k1    
  -> x2 ; k2*x3
  -> x3 ; k3*x1
  x1 -> ; d1*x1
  x2 -> ; d2*x2
  x3 -> ; d3*x3
  x1 + x2 -> ; gamma12*x1*x2
  
  # Equations
  x4 == 10 - x1
  x5' = 2 - x5
end

model main
  A: antithetic_controller()
end
